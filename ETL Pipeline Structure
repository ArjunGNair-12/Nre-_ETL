# extract.py
import pandas as pd
def extract(file_path):
    return pd.read_csv(file_path)

# transform.py
def transform(df):
    df = df.drop_duplicates()
    df['processed'] = True
    return df

# load.py
import sqlite3
def load(df, db_name):
    conn = sqlite3.connect(db_name)
    df.to_sql('final_table', conn, if_exists='replace', index=False)
    conn.close()
